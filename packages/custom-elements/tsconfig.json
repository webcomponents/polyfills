{
  "compilerOptions": {
    "incremental": true,
    "target": "es2019",
    "tsBuildInfoFile": "./buildcache",
    "module": "esnext",
    "declaration": true,
    "sourceMap": true,
    "outDir": "./src",
    "importHelpers": true,
    "strict": true,
    "noImplicitAny": true,
    "strictNullChecks": true,
    "strictFunctionTypes": true,
    "strictBindCallApply": true,
    "strictPropertyInitialization": true,
    "noImplicitThis": true,
    "alwaysStrict": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noImplicitReturns": true,
    "noFallthroughCasesInSwitch": true,
    "moduleResolution": "node"
  },
  "include": [
    "externs/**/*.d.ts",
    "node_modules/@webcomponents/html-imports/externs/html-imports.d.ts",
    "node_modules/@webcomponents/shadydom/externs/shadydom.d.ts",
    "ts_src/"
  ]
}
